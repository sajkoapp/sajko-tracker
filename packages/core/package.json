{
  "name": "@sajko/tracker",
  "version": "1.0.0",
  "description": "Core tracking library for SAJKO analytics and session replay",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "clean": "rm -rf dist node_modules",
    "prepare": "npm run build"
  },
  "keywords": [
    "analytics",
    "session-replay",
    "tracking",
    "sajko",
    "monitoring"
  ],
  "author": "SAJKO Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sajkoapp/sajko-tracker.git",
    "directory": "packages/core"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}